<?xml version="1.0"?>
<rss version="2.0">
	<channel>
		<title><%= user %>'s Facebook Links Feed</title>
		<% for(var i=0; i<links.length; i++) {%>
			<item>
				<title><%= links[i]["name"] %></title>
				<link><%= links[i]["link"] %></link>
				<pubDate><%= links[i]["created_time"] %></pubDate>
				<description><%= links[i]["description"] %></description>	
			</item>
	    <% } %>
	</channel>
</rss>